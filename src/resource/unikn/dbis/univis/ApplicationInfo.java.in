package unikn.dbis.univis;

/**
 * This <code>ApplicationInfo</code> provides version and compile time
 * strings.
 * It will be generated at build time.
 * <p/>
 * User: raedler
 * Date: 05.10.2006
 * Time: 17:26:58
 *
 * @author Roman R&auml;dle
 * @since UniVis Explorer 0.3.1.0
 */
public class ApplicationInfo {

    public static String getApplicationName() {
        return "@APPLICATION_NAME@";
    }

    public static String getVersion() {
        return "@APPLICATION_VERSION@";
    }

    public static String getVersionText() {
        return "@APPLICATION_VERSION_TXT@";
    }

    public static String getBuildtime() {
        return "@APPLICATION_BUILDTIME@";
    }

    public static String[] getAuthors() {
        return "@APPLICATION_AUTHORS@".split(",");
    }
}